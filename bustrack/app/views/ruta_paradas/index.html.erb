<h1>Listing ruta_paradas</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ruta_paradas.each do |ruta_parada| %>
      <tr>
        <td><%= ruta_parada.tipo %></td>
        <td><%= link_to 'Show', ruta_parada %></td>
        <td><%= link_to 'Edit', edit_ruta_parada_path(ruta_parada) %></td>
        <td><%= link_to 'Destroy', ruta_parada, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ruta parada', new_ruta_parada_path %>
